.address_box{height:120rpx;display:flex;align-items:center}.address_box .address_btn{width:300rpx;height:90rpx;line-height:90rpx;font-size:40rpx}.cart_box{margin-bottom:80rpx}.cart_box .cart_title{font-size:35rpx;border-top:1px solid #c17e79;border-bottom:1px solid #c17e79;padding:20rpx 10rpx;font-weight:600}.cart_content .cart_img{width:220rpx}.cart_content .goodsInfo_text_details{display:-webkit-box;overflow:hidden;-webkit-box-orient:vertical;-webkit-line-clamp:3}.cart_content .goodsInfo_text{width:480rpx;padding:30rpx}.cart_content .goodsInfo_text .goodsInfo_price_box{display:flex;justify-content:space-between;align-items:center;margin-top:100rpx}.cart_content .goodsInfo_text .goodsInfo_price{font-size:35rpx;color:var(--fontColor)}.cart_list{display:flex;align-items:center}.cart_bar{display:flex;justify-content:space-between;align-items:center;background-color:#fff;position:fixed;bottom:0;left:0;right:0}.cart_bar .bar_right{display:flex;align-items:center}.cart_bar .bar_right .bar_price{color:var(--fontColor);font-size:30rpx;margin-right:10rpx}.cart_bar .bar_right .settlement{width:160rpx;height:80rpx;background-color:#f8554d;color:#fff;display:flex;justify-content:center;align-items:center;font-size:30rpx;font-weight:600}.kongtishi{position:absolute;top:50%;right:50%;transform:translateX(50%);text-align:center}.kongtishi .iconfont{height:160rpx;background-color:rgba(201,123,59,0.2);line-height:160rpx;border-radius:50%;font-size:60rpx}.kongtishi .to_text{color:#df7474;margin:20rpx 0rpx}.kongtishi .go_category{background-color:royalblue;border-radius:10rpx;color:#fff;height:50rpx;line-height:50rpx}